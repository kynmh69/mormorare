            }
        },
        "handler.UserResponse": {
            "type": "object",
            "properties": {
                "birthday": {
                    "type": "string",
                    "example": "2006-01-02"
                },
                "email": {
                    "type": "string",
                    "example": "example@test.com"
                },
                "username": {
                    "type": "string",
                    "example": "test1"
                }
            }
        }
    }
